#include <iostream>
using namespace std;

int xton();
int main() {
  int x,n;

  cin >> x;
  cin >> n;

  cout << "raise " << x << " to the power of " << n << " is " << xton(x,n);
  
  return 0;
}

int xton(int x, int n) {
  //base case
  if(n == 0) {
    return x;
  }
  else { // recursive case
    return x * xton(x, n-1);
  }
  
}

