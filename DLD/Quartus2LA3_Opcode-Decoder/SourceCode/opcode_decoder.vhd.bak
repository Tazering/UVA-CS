LIBRARY ieee;
USE iee.std_logic_1164.all;

ENTITY opcode_decoder IS
	PORT
		(
			execute : IN STD_LOGIC;
			opcode : IN STD_LOGIC_VECTOR(2 DOWNTO 0);
			load : OUT STD_LOGIC;
			store : 	OUT STD_LOGIC;
			add : OUT STD_LOGIC;
			sub : OUT STD_LOGIC;
			inc : OUT STD_LOGIC;
			dec : OUT STD_LOGIC;
			bra : OUT STD_LOGIC;
			beq : OUT STD_LOGIC;
		);
	END opcode_decoder;